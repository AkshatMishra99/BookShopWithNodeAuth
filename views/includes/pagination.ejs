<section class="centered pagination">
	<!-- <% for(let i=0;i<pages;i++){ %> 
		<a href="?page=<%= i+1 %>" <% if((i+1)==page){ %> class="active" <% } else { %> class="<%= page %> " <% } %>> <%= i+1 %> </a>
	<% } %>  -->
	<% if(+page!==1){ %> <a href="?page=1">1</a><% } if(hasPreviousPage &&
	+previousPage!==1) {%>
	<a href="?page=<%= previousPage %> "><%= previousPage %> </a>
	<% } %>
	<a href="?page=<%= page %> " class="active"><%= page %></a>
	<% if(hasNextPage) {%>
	<a href="?page=<%= nextPage %> "><%= nextPage %></a><% } %> <%
	if(+nextPage!== +lastPage && +lastPage!==+page) { %>
	<a href="?page=<%= lastPage %>"><%= lastPage %></a><% } %>
</section>
